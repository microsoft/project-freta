{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Analysis",
  "type": "object",
  "required": [
    "analysis_info",
    "checks",
    "info",
    "initial_namespaces",
    "interrupt_table",
    "kernel_modules",
    "mounts",
    "networks",
    "syscall_tables",
    "tasks"
  ],
  "properties": {
    "analysis_info": {
      "$ref": "#/definitions/AnalysisInfo"
    },
    "block_cgroups": {
      "anyOf": [
        {
          "$ref": "#/definitions/BlockCgroups"
        },
        {
          "type": "null"
        }
      ]
    },
    "checks": {
      "$ref": "#/definitions/Checks"
    },
    "info": {
      "$ref": "#/definitions/Info"
    },
    "initial_namespaces": {
      "$ref": "#/definitions/Namespaces"
    },
    "interrupt_table": {
      "$ref": "#/definitions/IDT"
    },
    "kernel_modules": {
      "$ref": "#/definitions/Modules"
    },
    "mounts": {
      "$ref": "#/definitions/Mounts"
    },
    "networks": {
      "$ref": "#/definitions/Networks"
    },
    "syscall_tables": {
      "$ref": "#/definitions/SyscallTables"
    },
    "tasks": {
      "$ref": "#/definitions/Tasks"
    }
  },
  "definitions": {
    "Address": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "None"
          ]
        },
        {
          "type": "object",
          "required": [
            "IPV4"
          ],
          "properties": {
            "IPV4": {
              "type": "string",
              "format": "ipv4"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "IPV6"
          ],
          "properties": {
            "IPV6": {
              "type": "string",
              "format": "ipv6"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "AnalysisInfo": {
      "type": "object",
      "required": [
        "analysis_version",
        "symbols_version",
        "tags"
      ],
      "properties": {
        "analysis_version": {
          "type": "string"
        },
        "symbols_version": {
          "type": "string"
        },
        "tags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "ArpTable": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Entry"
      }
    },
    "BlockCgroups": {
      "type": "object",
      "required": [
        "cgroups"
      ],
      "properties": {
        "cgroups": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Cgroup"
          }
        },
        "init_cgroup": {
          "anyOf": [
            {
              "$ref": "#/definitions/Cgroup"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "CallbackSymbol": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "Kernel"
          ],
          "properties": {
            "Kernel": {
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "Module"
          ],
          "properties": {
            "Module": {
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "Cgroup": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "count": {
          "type": [
            "integer",
            "null"
          ],
          "format": "int32"
        },
        "id": {
          "type": [
            "integer",
            "null"
          ],
          "format": "int32"
        },
        "level": {
          "type": [
            "integer",
            "null"
          ],
          "format": "int32"
        },
        "name": {
          "type": "string"
        },
        "subsystem_name": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "CgroupNamespace": {
      "type": "object",
      "required": [
        "cgroup_addr",
        "user_namespace_addr"
      ],
      "properties": {
        "cgroup_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "task_count": {
          "type": [
            "integer",
            "null"
          ],
          "format": "int32"
        },
        "user_namespace_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        }
      }
    },
    "Checks": {
      "type": "object",
      "properties": {
        "duplicate_credentials": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DuplicateCredential"
          }
        },
        "duplicate_imports": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DuplicateImport"
          }
        },
        "file_handlers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/FileHandler"
          }
        },
        "functions": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Function"
          }
        },
        "idt": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/IdtEntry"
          }
        },
        "netfilter_issues": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/NetfilterIssue"
          }
        },
        "syscalls": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Syscall"
          }
        },
        "unexpected_module_notifiers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UnexpectedNotifier"
          }
        },
        "unlinked_modules": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/UnlinkedModule"
          }
        }
      }
    },
    "Cred": {
      "type": "object",
      "required": [
        "egid",
        "euid",
        "gid",
        "sgid",
        "suid",
        "uid"
      ],
      "properties": {
        "egid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "euid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "gid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "sgid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "suid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "uid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    },
    "CssSet": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Cgroup"
      }
    },
    "DuplicateCredential": {
      "type": "object",
      "required": [
        "address",
        "dup_pid",
        "field",
        "pid"
      ],
      "properties": {
        "address": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "dup_pid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "field": {
          "type": "string"
        },
        "pid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    },
    "DuplicateImport": {
      "type": "object",
      "required": [
        "paths",
        "pid"
      ],
      "properties": {
        "paths": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "pid": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    },
    "Entry": {
      "type": "object",
      "required": [
        "addr",
        "id"
      ],
      "properties": {
        "addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "id": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "implementation": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "anyOf": [
            {
              "$ref": "#/definitions/IdtName"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "File": {
      "type": "object",
      "required": [
        "fd",
        "file_addr",
        "offset",
        "path"
      ],
      "properties": {
        "fd": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "file_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "inode": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "offset": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "path": {
          "type": "string"
        },
        "size": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "FileHandler": {
      "type": "object",
      "required": [
        "addr",
        "area",
        "disassembly",
        "function",
        "hook_type"
      ],
      "properties": {
        "addr": {
          "description": "Address of the hooked function",
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "area": {
          "type": "string"
        },
        "disassembly": {
          "description": "disassembly of the hooked function",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "function": {
          "type": "string"
        },
        "hook_type": {
          "description": "type of hook",
          "type": "string"
        },
        "target_addr": {
          "description": "calculated address for the destination of the hook",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0.0
        },
        "target_disassembly": {
          "description": "disassembly of the destination for the hooked function",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "target_module": {
          "description": "symbol name for the destination for the hooked function if known",
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Flags": {
      "type": "string",
      "enum": [
        "READ_ONLY",
        "NO_SUID",
        "NO_DEV",
        "NO_EXEC",
        "SYNCHRONOUS",
        "REMOUNT",
        "ALLOW_MANDITORY_LOCKS",
        "DIR_MODS_ARE_SYNCHRONOUS",
        "NO_ATIME",
        "NO_DIR_ATIME",
        "BIND",
        "MOVE",
        "REC",
        "SILENT",
        "POSIX_ACL",
        "UNBINDABLE",
        "PRIVATE",
        "SLAVE",
        "SHARED",
        "REL_ATIME",
        "KERN_MOUNT",
        "I_VERSION",
        "STRICT_ATIME",
        "NO_SEC",
        "BORN",
        "ACTIVE",
        "NO_USER",
        "__NOT_USED"
      ]
    },
    "Function": {
      "type": "object",
      "required": [
        "addr",
        "disassembly",
        "hook_type",
        "name"
      ],
      "properties": {
        "addr": {
          "description": "Address of the hooked function",
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "disassembly": {
          "description": "disassembly of the hooked function",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "hook_type": {
          "description": "type of hook",
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "target_addr": {
          "description": "calculated address for the destination of the hook",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0.0
        },
        "target_disassembly": {
          "description": "disassembly of the destination for the hooked function",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "target_module": {
          "description": "symbol name for the destination for the hooked function if known",
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "IDT": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Entry"
      }
    },
    "IdtEntry": {
      "type": "object",
      "required": [
        "addr",
        "disassembly",
        "hook_type",
        "id"
      ],
      "properties": {
        "addr": {
          "description": "Address of the hooked function",
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "disassembly": {
          "description": "disassembly of the hooked function",
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "hook_type": {
          "description": "type of hook",
          "type": "string"
        },
        "id": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "target_addr": {
          "description": "calculated address for the destination of the hook",
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0.0
        },
        "target_disassembly": {
          "description": "disassembly of the destination for the hooked function",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "target_module": {
          "description": "symbol name for the destination for the hooked function if known",
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "IdtName": {
      "type": "string",
      "enum": [
        "divide_by_zero",
        "non_maskable_interrupt",
        "breakpoint",
        "overflow",
        "bound_range_exceeded",
        "invalid_opcode",
        "device_not_available",
        "double_fault",
        "coprocessor_segment_overrun",
        "invalid_tss",
        "segment_not_present",
        "general_protection_fault",
        "page_fault",
        "spurious_interrupt",
        "floating_point_exception",
        "alignment_check",
        "machine_check",
        "simd_floating_point_exception",
        "iret_exception"
      ]
    },
    "Inet": {
      "type": "object",
      "required": [
        "dst_addr",
        "dst_port",
        "ip_proto",
        "socket_family",
        "socket_state",
        "socket_type",
        "src_addr",
        "src_port"
      ],
      "properties": {
        "dst_addr": {
          "$ref": "#/definitions/Address"
        },
        "dst_port": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0.0
        },
        "ip_proto": {
          "$ref": "#/definitions/ip_proto"
        },
        "socket_family": {
          "$ref": "#/definitions/sock_family"
        },
        "socket_state": {
          "$ref": "#/definitions/tcp_states"
        },
        "socket_type": {
          "$ref": "#/definitions/sock_types"
        },
        "src_addr": {
          "$ref": "#/definitions/Address"
        },
        "src_port": {
          "type": "integer",
          "format": "uint16",
          "minimum": 0.0
        }
      }
    },
    "Info": {
      "type": "object",
      "required": [
        "banner",
        "kernel_aslr_offset",
        "kernel_pml4",
        "machine",
        "memory_info",
        "vm_info"
      ],
      "properties": {
        "banner": {
          "type": "string"
        },
        "kernel_aslr_offset": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "kernel_pml4": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "machine": {
          "$ref": "#/definitions/UtsName"
        },
        "memory_info": {
          "$ref": "#/definitions/MemoryInfo"
        },
        "vm_info": {
          "$ref": "#/definitions/Info"
        }
      }
    },
    "MappedFile": {
      "type": "object",
      "required": [
        "exported_name",
        "name"
      ],
      "properties": {
        "exported_name": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "MemoryInfo": {
      "type": "object",
      "required": [
        "swap_pages",
        "total_swap_pages"
      ],
      "properties": {
        "swap_pages": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "total_ram": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "total_swap_pages": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    },
    "MntNamespace": {
      "type": "object",
      "required": [
        "addr",
        "path",
        "root_addr"
      ],
      "properties": {
        "addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "path": {
          "type": "string"
        },
        "root_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "user_namespace_addr": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint",
          "minimum": 0.0
        }
      }
    },
    "Module": {
      "type": "object",
      "required": [
        "addr",
        "core_addr",
        "core_size",
        "init_addr",
        "init_size",
        "name",
        "source",
        "symbols"
      ],
      "properties": {
        "addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "args": {
          "type": [
            "string",
            "null"
          ]
        },
        "core_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "core_artifact_name": {
          "type": [
            "string",
            "null"
          ]
        },
        "core_size": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "init_addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "init_artifact_name": {
          "type": [
            "string",
            "null"
          ]
        },
        "init_size": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "name": {
          "type": "string"
        },
        "source": {
          "$ref": "#/definitions/ModuleSource"
        },
        "srcversion": {
          "type": [
            "string",
            "null"
          ]
        },
        "symbols": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ModuleSymbol"
          }
        },
        "version": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "ModuleNotifier": {
      "type": "object",
      "required": [
        "callback_address",
        "priority"
      ],
      "properties": {
        "callback_address": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "callback_name": {
          "anyOf": [
            {
              "$ref": "#/definitions/CallbackSymbol"
            },
            {
              "type": "null"
            }
          ]
        },
        "priority": {
          "type": "integer",
          "format": "int64"
        }
      }
    },
    "ModuleSource": {
      "type": "string",
      "enum": [
        "ModulesList",
        "ModuleBugList"
      ]
    },
    "ModuleSymbol": {
      "type": "object",
      "required": [
        "addr",
        "name"
      ],
      "properties": {
        "addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "name": {
          "type": "string"
        }
      }
    },
    "Modules": {
      "type": "object",
      "required": [
        "modules",
        "notifiers"
      ],
      "properties": {
        "modules": {
          "$ref": "#/definitions/ModulesList"
        },
        "modules_disabled": {
          "type": [
            "integer",
            "null"
          ],
          "format": "int64"
        },
        "notifiers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ModuleNotifier"
          }
        }
      }
    },
    "ModulesList": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Module"
      }
    },
    "Mount": {
      "type": "object",
      "required": [
        "flags",
        "mount_type",
        "name",
        "path"
      ],
      "properties": {
        "flags": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Flags"
          }
        },
        "mount_type": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      }
    },
    "Mounts": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Mount"
      }
    },
    "Namespaces": {
      "type": "object",
      "required": [
        "addr",
        "count"
      ],
      "properties": {
        "addr": {
          "type": "integer",
          "format": "uint",
          "minimum": 0.0
        },
        "cgroup": {
          "anyOf": [
            {
              "$ref": "#/definitions/CgroupNamespace"
            },
            {
              "type": "null"
            }
          ]
        },
        "count": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "mnt": {
          "anyOf": [
            {
              "$ref": "#/definitions/MntNamespace"
            },
            {
              "type": "null"
            }
          ]
        },
        "net": {
          "anyOf": [
            {
              "$ref": "#/definitions/NetNamespace"
            },
            {
              "type": "null"
            }
          ]
        },
        "pid": {
          "anyOf": [
            {
              "$ref": "#/definitions/PidNamespace"
            },
            {
              "type": "null"
            }
          ]
